{
	"name": "Cyber Citizenship Coefficient",
	"description": "Build an Chrome Plugin Extension",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "main.js"
	},
	"permissions": [
		"contextMenus",
		"tabs"
	],
	"content_scripts": [
		{
			"js": [
				"contentScript.js"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"getElement.js"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"key": ""
}